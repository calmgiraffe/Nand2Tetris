/** Initializes a new Square Dance game and starts running it. */
class Main {
    function void main() {
        var Square square;
        let square = Square.new(5, 6, 50);
        //do square.draw();

        
        do square.draw();
        do Sys.wait(1000);
        do square.erase();
        do Sys.wait(1000);
        do square.draw();
        do Sys.wait(1000);
        do square.erase();
        do Sys.wait(1000);
        

        do square.dispose();
        return;
    }
}
